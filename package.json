{
  "name": "react-parallax-hover",
  "version": "0.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/kebirek/react-parallax-hover.git"
  },
  "keywords": [
    "react",
    "react-component",
    "parallax",
    "hover"
  ],
  "description": "A pointer driven hover effect that rotates images in place with subtle parallax effects",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist && mkdir ./dist",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --colors",
    "start": "npm run clean && npm run watch",
    "build": "npm run clean && ./node_modules/.bin/babel ./src/parallax-hover.js -o ./dist/index.js",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Juan Camacho <camachojuan@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.0.14",
    "babel-cli": "^6.0.15",
    "babel-core": "^6.0.14",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.0.14",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-react": "^6.0.14",
    "css-loader": "^0.21.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-import": "^0.8.1",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "node-sass": "^3.4.1",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
